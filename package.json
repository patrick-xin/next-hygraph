{
  "name": "next-hygraph",
  "version": "1.0.0",
  "description": "Next13 with Hygraph CMS",
  "keywords": [
    "tailwindcss",
    "nextjs",
    "hygraph",
    "CMS",
    "react"
  ],
  "author": {
    "email": "patrick.xin.dev@gmail.com",
